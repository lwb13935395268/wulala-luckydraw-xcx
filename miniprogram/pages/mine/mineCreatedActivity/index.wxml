<!--pages/mine/mineCreatedActivity/index.wxml-->

<tab titleList="{{['正在进行','已结束']}}">
    <view slot='正在进行' class="wrap gap-tb-20">
        <view class="create-btn" bindtap="navgator" data-path="selectActivity/index">
            去创建一个活动
        </view>
        <view class="" wx:for="{{myActicityList}}" wx:key="index">
          <activity status="0" startDate="{{item.startDate}}" endDate="{{item.endDate}}" title="{{item.activityTitle}}" imageUrl="{{item.imgFileId}}">
          </activity>
        </view>
    </view>
    <view slot='已结束' class="wrap ptb-20">
        <activity status="2"></activity>
        <activity status="2"></activity>
    </view>
</tab>
