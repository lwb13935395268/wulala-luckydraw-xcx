<!--pages/mine/mineActivity/index.wxml-->
<tab titleList="{{titleList}}" bind:myevent="onMyEvent">
    <view slot='正在进行' class="wrap gap-tb-20">
        <activity title="{{item.activityTitle}}" startDate="{{item.startDate}}" endDate="{{item.endDate}}" imageUrl="{{item.imgFileId}}" wx:for="{{activityList}}" wx:key="_id" status=""></activity>
    </view>
    <view slot='已结束' class="wrap gap-tb-20">
        <activity title="{{item.activityTitle}}" startDate="{{item.startDate}}" endDate="{{item.endDate}}" imageUrl="{{item.imgFileId}}" wx:for="{{overActivityList}}" wx:key="_id" status=""></activity>
    </view>
</tab>